e(activeData.title);\n    setContent(activeData.content);\n  }, [activeData]);\n\n  const handleSave = async (e) => {\n    setSaving(true);\n    let completeFlag = complete;\n    const target = e.target.innerHTML;\n    if (target === \"Incomplete\" || target === \"Complete\") {\n      setComplete(!complete);\n      completeFlag = !complete;\n    }\n    await saveData({\n      variables: {\n        itemId: activeData.id,\n        title: title || \"ADD TITLE\",\n        content: content,\n        completed: completeFlag !== \"undefined\" ? completeFlag : complete,\n      },\n    });\n    const newTodo = {\n      completed: completeFlag !== \"undefined\" ? completeFlag : complete,\n      title: title || \"ADD TITLE\",\n      content: content,\n      id: activeData.id,\n      userId: userId,\n      __typename: \"Item\",\n    };\n    dispatch(updateTodos(newTodo));\n  };\n\n  const handleSetTitle = (e) => {\n    setSaving(false);\n    if (e.target.value.length < 80) {\n      setTitle(e.target.value);\n    } else return null;\n  };\n  const handleSetContent = (e) => {\n    setSaving(false);\n    setContent(e.target.value);\n  };\n  console.log(saving);\n  return (\n    <div className={classes.detailContent}>\n      <header className={classes.itemStatus}>\n        {complete ? (\n          <Button onClick={(e) => handleSave(e)} className={classes.complete}>\n            Complete\n          </Button>\n        ) : (\n          <Button onClick={(e) => handleSave(e)} className={classes.inComplete}>\n            Incomplete\n          </Button>\n        )}\n      </header>\n      <Typography gutterBottom variant=\"h6\">\n        <InputBase\n          name=\"title\"\n          autoComplete=\"off\"\n          autoFocus={true}\n          className={classes.titleEditable}\n          onChange={(e) => handleSetTitle(e)}\n          onBlur={(e) => handleSave(e)}\n          value={title || \"\"}\n          inputProps={{ \"aria-label\": \"naked\" }}\n        />\n      </Typography>\n      <Divider style={{ backgroundColor: \"grey\", marginBottom: \"20px\" }} />\n      <Typography gutterBottom variant=\"h6\">\n        <TextField\n          multiline={true}\n          rows={30}\n          InputProps={{\n            className: classes.contentEditable,\n            disableUnderline: true,\n          }}\n          autoComplete=\"off\"\n          name=\"content\"\n          fullWidth={true}\n          onChange={(e) => handleSetContent(e)}\n          onBlur={(e) => handleSave(e)}\n          value={content || \"\"}\n          inputProps={{ \"aria-label\": \"naked\" }}\n        />\n      </Typography>\n    </div>\n  );\n};;\n\nexport default TodoDetails;\n\n","/home/kmcgee92/projects/TodoApp/client/src/graphql/mutations/updateItem.js",[],"/home/kmcgee92/projects/TodoApp/client/src/Components/TodoInterface/TodoList/Todo/Todo.js",[],"/home/kmcgee92/projects/TodoApp/client/src/graphql/mutations/signup.js",[],"/home/kmcgee92/projects/TodoApp/client/src/Components/TodoInterface/Header/Delete.js",[],"/home/kmcgee92/projects/TodoApp/client/src/Components/TodoInterface/Header/Add.js",[],"/home/kmcgee92/projects/TodoApp/client/src/Components/Auth/Login.js",[],"/home/kmcgee92/projects/TodoApp/client/src/Components/Auth/Logout.js",[],"/home/kmcgee92/projects/TodoApp/client/src/graphql/mutations/deleteItem.js",[],"/home/kmcgee92/projects/TodoApp/client/src/graphql/mutations/createItem.js",[],"/home/kmcgee92/projects/TodoApp/client/src/graphql/mutations/login.js",[],{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","severity":1,"message":"136","line":15,"column":10,"nodeType":"137","messageId":"138","endLine":15,"endColumn":13},"no-native-reassign",["139"],"no-negated-in-lhs",["140"],"no-unused-vars","'set' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]